{
  "name": "@any-reader/vscode",
  "displayName": "any-reader",
  "description": "摸鱼,自定义规则多站点聚合搜索阅读小说、漫画。包含JS规则解析库和VSCode插件。支持本地小说 TXT、EPUB",
  "icon": "resources/icon.png",
  "version": "1.4.1",
  "publisher": "aooiu",
  "qna": "https://github.com/aooiuu/any-reader/issues",
  "bugs": {
    "url": "https://github.com/aooiuu/any-reader/issues",
    "email": "aooiu@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aooiuu/any-reader"
  },
  "keywords": [
    "阅读",
    "小说",
    "漫画",
    "摸鱼",
    "reader",
    "eso"
  ],
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:any-reader",
    "onWebviewPanel:any-reader",
    "onCommand:any-reader.getBookSource",
    "onCommand:any-reader.searchBook",
    "onCommand:any-reader.searchBookByRule",
    "onCommand:any-reader.discover",
    "onCommand:any-reader.home",
    "onCommand:any-reader.gamePlay",
    "onCommand:any-reader.star",
    "onCommand:any-reader.unstar",
    "onCommand:any-reader.openLocalBookDir"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "build:rename": "esno ./scripts/publish.cjs",
    "build:vsce": "vsce package --no-dependencies",
    "build": "npm-run-all build:rename build:vsce",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "any-reader.getBookSource",
        "title": "刷新书源",
        "icon": "$(refresh)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.searchBook",
        "title": "搜索",
        "icon": "$(search)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.searchBookByRule",
        "title": "搜索",
        "icon": "$(search)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.discover",
        "title": "discover",
        "icon": "$(flame)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.home",
        "title": "Home",
        "icon": "$(home)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.gamePlay",
        "title": "Game Play",
        "icon": "$(game)",
        "category": "any-reader"
      },
      {
        "command": "any-reader.star",
        "title": "star",
        "icon": "$(star)"
      },
      {
        "command": "any-reader.unstar",
        "title": "unstar",
        "icon": "$(star-full)"
      },
      {
        "command": "any-reader.openLocalBookDir",
        "title": "open local book dir",
        "icon": "$(folder-opened)",
        "category": "any-reader"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "any-reader",
          "title": "any-reader",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "any-reader": [
        {
          "id": "any-reader-book",
          "name": "阅读"
        },
        {
          "id": "any-reader-source",
          "name": "规则"
        },
        {
          "id": "any-reader-webview",
          "name": "view",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "any-reader.discover",
          "when": "view == any-reader-source",
          "group": "navigation"
        },
        {
          "command": "any-reader.searchBook",
          "when": "view == any-reader-source",
          "group": "navigation"
        },
        {
          "command": "any-reader.getBookSource",
          "when": "view == any-reader-source",
          "group": "navigation"
        },
        {
          "command": "any-reader.home",
          "when": "view == any-reader-source",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "any-reader.star",
          "when": "view == any-reader-book && viewItem == star",
          "group": "inline"
        },
        {
          "command": "any-reader.unstar",
          "when": "view == any-reader-book && viewItem == unstar",
          "group": "inline"
        },
        {
          "command": "any-reader.searchBookByRule",
          "when": "view == any-reader-source && viewItem != GAME",
          "group": "inline"
        },
        {
          "command": "any-reader.gamePlay",
          "when": "view == any-reader-source && viewItem == GAME",
          "group": "inline"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "any-reader-source",
        "contents": "[如何添加规则](https://aooiuu.github.io/any-reader/vsc/#%E9%85%8D%E7%BD%AE%E8%A7%84%E5%88%99)\n[提建议 | 提BUG](https://github.com/aooiuu/any-reader/issues)"
      },
      {
        "view": "any-reader-book",
        "contents": "搜索或者查看章节列表后数据会出现在这里 \n[使用文档](https://aooiuu.github.io/any-reader/vsc/)\n[提建议 | 提BUG](https://github.com/aooiuu/any-reader/issues)"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "AnyReader",
      "properties": {
        "any-reader.read-page-mode": {
          "type": "string",
          "default": "Editor",
          "enum": [
            "Editor",
            "Sidebar"
          ],
          "description": "阅读页模式： Editor=编辑器面板 Sideba=侧边栏"
        }
      }
    }
  },
  "devDependencies": {
    "@any-reader/core": "workspace:*",
    "@any-reader/shared": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "bufferutil": "^4.0.8",
    "easy-post-message": "^0.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^8.0.1",
    "explorer-opener": "^1.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "qs": "^6.12.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "utf-8-validate": "^6.0.4",
    "uuid": "^9.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
